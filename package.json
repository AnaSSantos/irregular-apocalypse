{
  "private": true,
  "name": "irregular-apocalypse",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "tslint src",
    "format": "prettier-standard 'src/**/*.{js,ts,tsx,json,css,md}'",
    "test": "jest"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,ts,tsx,json,css,md}": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.0",
    "jest": "^22.0.6",
    "jest-cli": "^22.0.6",
    "lint-staged": "^6.0.0",
    "preact-cli": "^2.0.1",
    "preact-cli-plugin-typescript": "^0.2.2",
    "preact-testing-library": "^0.0.1-semantically-released",
    "prettier-standard": "^8.0.0",
    "ts-jest": "^22.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-react": "^3.3.3"
  },
  "dependencies": {
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.7"
  }
}
