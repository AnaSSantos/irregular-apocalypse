{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "h",
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["./cypress/**"]
}
